<?xml version='1.0' encoding='UTF-8'?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
    version="1.2">
  
  <application>
    <view-handler>com.sun.facelets.FaceletViewHandler</view-handler>    
  </application>
  
  <navigation-rule>
    <description/>
    <from-view-id>/cmmdcInput.xhtml</from-view-id>
    <navigation-case>
      <description/>
      <from-outcome>OK</from-outcome>
      <to-view-id>/cmmdcOutput.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>

  <navigation-rule>
    <description/>
    <from-view-id>/cmmdcInput1.xhtml</from-view-id>
    <navigation-case>
      <description/>
      <from-outcome>OK</from-outcome>
      <to-view-id>/cmmdcOutput1.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>
  
  <navigation-rule>
    <description/>
    <from-view-id>/appjudInput.xhtml</from-view-id>
    <navigation-case>
      <description/>
      <from-outcome>regaseste </from-outcome>
      <to-view-id>/appjudOutput.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>
  
